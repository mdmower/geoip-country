{
  "name": "geoip-web-api",
  "version": "1.1.2",
  "description": "An AMP-GEO compatible location web API",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "build-release": "npm run lint && npm run compile-release",
    "clean": "node build/clean.js",
    "compile": "tsc --build tsconfig.json",
    "compile-release": "tsc --build tsconfig.release.json",
    "debug": "node --inspect-brk lib/cli.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "start": "node lib/cli.js"
  },
  "bin": {
    "geoip-web-api": "lib/cli.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/geoip-web-api.git"
  },
  "keywords": [
    "geoip",
    "amp-geo",
    "location",
    "maxmind"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/geoip-web-api/issues"
  },
  "homepage": "https://github.com/mdmower/geoip-web-api#readme",
  "dependencies": {
    "express": "^4.17.1",
    "ip2ldb-reader": "^1.5.1",
    "maxmind": "^4.3.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^9.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
