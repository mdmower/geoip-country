{
  "name": "geoip-web-api",
  "version": "1.0.4",
  "description": "An AMP-GEO compatible location web API",
  "main": "lib/index.js",
  "scripts": {
    "debug": "node --nolazy lib/cli.js",
    "start": "node lib/cli.js",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "npm run lint && tsc",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "geoip-web-api": "lib/cli.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/geoip-web-api.git"
  },
  "keywords": [
    "geoip",
    "amp-geo",
    "location",
    "maxmind"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/geoip-web-api/issues"
  },
  "homepage": "https://github.com/mdmower/geoip-web-api#readme",
  "dependencies": {
    "express": "^4.17.1",
    "maxmind": "^4.1.4",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.6.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsdoc": "^30.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
